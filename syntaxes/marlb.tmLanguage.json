{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "marlb",
	"fileTypes": [".marble"],
	"scopeName": "source.marble",
	"patterns": [
		{
			"match": "\\+|-",
			"name": "keyword.operator.arithmetic.marlb"
		},
		{
			"match": "<|>",
			"name": "storage.type.marlb"
		},
		{
			"match": ":|!",
			"name": "keyword.control.marlb"
		},
		{
			"match": "\".*",
			"name": "comment"
		},
		{
			"match": "q|w|e|r|u|i|o|p|a|s|d|j|k",
			"name": "variable.parameter"
		},
		{
			"match": "[^qweruiopasdjk!:<>+-]",
			"name": "comment"
		}
	]
}